<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SongbookBeta"
        tools:targetApi="31">
        <activity
            android:name=".AddSong"
            android:exported="false"
            android:label="@string/title_activity_add_song"
            android:theme="@style/Theme.SongbookBeta" />
        <activity
            android:name=".RandomSong"
            android:exported="false"
            android:label="@string/title_activity_random_song"
            android:theme="@style/Theme.SongbookBeta" />
        <activity
            android:name=".Settings"
            android:exported="false"
            android:label="@string/title_activity_settings"
            android:theme="@style/Theme.SongbookBeta" />
        <activity
            android:name=".BonfireSongs"
            android:exported="false"
            android:label="@string/title_activity_bonfire_songs"
            android:theme="@style/Theme.SongbookBeta" />
        <activity
            android:name=".FavouriteSongs"
            android:exported="false"
            android:label="@string/title_activity_favourite_songs"
            android:theme="@style/Theme.SongbookBeta" />
        <activity
            android:name=".AllSongs"
            android:exported="false"
            android:label="@string/title_activity_all_songs"
            android:theme="@style/Theme.SongbookBeta" />
        <activity
            android:name=".LastSongs"
            android:exported="false"
            android:label="@string/title_activity_last_songs"
            android:theme="@style/Theme.SongbookBeta" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.SongbookBeta">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>